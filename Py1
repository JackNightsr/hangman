x = int(input("Enter x coordinate: "))
y = int(input("Enter y coordinate: "))

if y > 0 and x < 0:
	print("Точка ({0},{1}) не належить графіку функції".format(x, y))
elif x**2 + y**2 >= 9 and abs(x) <= 3 and abs(y) <= 3:
	print("Точка ({0},{1}) належить графіку функції".format(x, y))
else:
	print("Точка ({0},{1}) належить графіку функції".format(x, y))
